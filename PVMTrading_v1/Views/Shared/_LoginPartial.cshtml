@using Microsoft.AspNet.Identity
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>SB Admin - Start Bootstrap Template</title>
    <!-- Bootstrap core CSS-->
    @Styles.Render("~/bundles/corecss")
    <!-- Custom fonts for this template-->
    @Styles.Render("~/bundles/font-awesome")
    <!-- Custom styles for this template-->
    @Styles.Render("~/bundles/customstyle")
    <link rel="stylesheet" href="~/Content/background.css">
    <script src="~/Content/jquery.min.js"></script>
    <script src="~/Content/bootstrap.min.js"></script>
</head>
<body class="bg-light">
<div class="carousel slide carousel-fade" data-ride="carousel">

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
        <div class="item active">
        </div>
        <div class="item">
        </div>
        @*<div class="item">
        </div>*@
    </div>
</div>
    


            


<div>
    @RenderBody()
</div>
<!-- Bootstrap core JavaScript-->
@Scripts.Render("~/bundles/core")

<!-- Core plugin JavaScript-->
@Scripts.Render("~/bundles/coreplugin")
@RenderSection("scripts", required: false)

</body>
</html>

